{
    "name": "bizdirenepal/demo-yii2",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "vlucas/phpdotenv": "^5.5",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-bootstrap": "^2.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "yiisoft/yii2-swiftmailer": "^2.0",
        "bower-asset/adminlte": "~2.3.0",
        "codezeen/yii2-adminlte": "~1.2.0",
        "codezeen/yii2-tinymce": "~1.0.0",
        "kartik-v/yii2-grid": "^3.5",
        "kartik-v/yii2-widgets": "^3.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "^2.1",
        "yiisoft/yii2-gii": "^2.2",
        "yiisoft/yii2-faker": "^2.0",
        "codeception/base": "^3.1",
        "codeception/verify": "^1.5",
        "codeception/specify": "^1.4"
    },
    "config": {
        "fxp-asset": {
            "enabled": false,
            "vcs-driver-options": {
                "github-no-api": true
            },
            "git-skip-update": "2 days",
            "pattern-skip-version": "(-build|-patch)",
            "optimize-with-installed-packages": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
